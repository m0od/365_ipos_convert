[Unit]
Description=Celery Service webtool
After=network.target

[Service]
User=blackwings
WorkingDirectory=/home/blackwings/webtool/webgui
Environment="PATH=/home/blackwings/webtool/bin"
ExecStart=/home/blackwings/webtool/bin/celery -A common_celery.background worker -E --concurrency=32

[Install]
WantedBy=default.target